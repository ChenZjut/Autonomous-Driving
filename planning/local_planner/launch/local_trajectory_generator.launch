<launch>
    <node pkg="local_planner" type="local_trajectory_generator_node" name="local_trajectory_generator_node" output="screen">
        <param name="horizonDistance" value="20.0"/>
        <param name="rollOutNumber" value="14"/>
        <param name="rollOutDensity" value="0.3"/>
        <param name="horizontalSafetyDistancel" value="1"/>
        <param name="verticalSafetyDistance" value="1"/>
        <param name="minFollowingDistance" value="50"/>
        <param name="lateralSkipDistance" value="6.0"/>
        <param name="width" value="0.8"/>
        <param name="length" value="1.5"/>
        <param name="wheel_base" value="0.80"/>
    </node>
</launch> 
